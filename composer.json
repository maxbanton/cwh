{
    "name": "maxbanton/cwh",
    "description": "AWS CloudWatch Handler for Monolog library",
    "type": "library",
    "license": "MIT",
    "keywords": ["aws", "cloudwatch", "monolog"],
    "authors": [
        {
            "name": "Max Leonov",
            "email": "hi@maxleonov.pw"
        }
    ],
    "autoload": {
        "psr-4": {
            "Maxbanton\\Cwh\\": "src"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "monolog/monolog": "^1.20",
        "aws/aws-sdk-php": "^3.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "^1.0",
        "maxbanton/dd": "^1.0"
    },
    "scripts": {
      "fix-syntax": "vendor/bin/phpcbf --standard=psr2 src/",
      "check-syntax": "vendor/bin/phpcs --standard=psr2 --ignore=Tests src/"
    }
}
